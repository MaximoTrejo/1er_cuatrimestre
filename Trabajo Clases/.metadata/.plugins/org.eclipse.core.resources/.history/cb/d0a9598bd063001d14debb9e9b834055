#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int *a;
	int *b;
	int c;

	*a=&2;
	*b=&3;
	c=4;

	a=b;
	a=&c;


printf("%d,%d,%d",a,b,c);
	return EXIT_SUCCESS;
}
