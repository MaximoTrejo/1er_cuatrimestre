#include <stdio.h>
#include <stdlib.h>
#include "menu.h"

int main(void) {
	setbuf(stdout, NULL);
	int opcion = 0;
	do{
		opcionesMenu();
		utn_getInt(&opcion, "Ingrese la opcion", "Error", 1, 11, REINTENTOS){
			switch(opcion){
				case 1:
					break;
			}
		}
	}while(opcion != 11);

	return EXIT_SUCCESS;
}
