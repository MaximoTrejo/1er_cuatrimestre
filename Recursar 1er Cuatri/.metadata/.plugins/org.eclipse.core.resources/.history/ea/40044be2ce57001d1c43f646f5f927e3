#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int vector[5]={5,3,2,1,6};

	int *aux=&vector[0];

		for ( i = 0 ; i < 5-1 ; i ++ )
		{
		    for ( j = i + 1 ; j < 5 ; j ++ ){
		        if ( numeros [ i ] < numeros [ j ] ){

		             aux = numeros [ i ] ;
		             numeros [ i ] = numeros [ j ] ;
		             numeros [ j ] = aux ;
		        }
		    }
		}



	return EXIT_SUCCESS;
}



int ordenarEnteros(int * puntero, int vector[]){
	int retorno;

	for ( int i = 0 ; i < 5 ; i ++ ){
		*puntero = vector [ i ] ;
		vector [ i ] = numeros [ j ] ;
		vector [ j ] = *puntero ;
	}

	return retorno;
}



int swap(int * vA ,int *vB){
	int retorno=-1;
	int * pAux;

	*pAux = *vA ;
	*vA = *vB;
	*vB = *pAux ;

	return retorno;
}
